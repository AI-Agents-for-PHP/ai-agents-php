{
    "name": "caiquebispo/ai-agents-php",
    "description": "A PHP library for building and managing AI agents with various chat models and functionalities.",
    "keywords": ["php", "laravel", "ai", "chatgpt", "openai", "azure", "anthropic", "twilio", "weather", "agents"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CaiqueBispo\\AiAgentsPhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Caique Bispo",
            "email": "caiquebispo0023@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "openai-php/client": ">=0.8.0",
        "twilio/sdk": "^8.3",
        "guzzlehttp/guzzle": "^7.8",
        "yethee/tiktoken": "^0.5.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
